<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">

    <item android:state_pressed="true"> <!-- presionado -->
        <shape android:shape="rectangle">
            <corners android:radius="60dp" />
            <solid android:color="@color/colorNormalE" />
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <corners android:radius="60dp" />
            <solid android:color="@color/colorPressed" />
        </shape>
    </item><!-- en foco -->
    <item android:drawable="@color/colorNormal" tools:ignore="StateListReachable" /> <!-- por defecto -->

</selector>
